{
	"name": "gear-next",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "yarn build:css && next build",
		"export": "yarn build && next export",
		"start": "next start",
		"lint": "next lint",
		"dev:css": "npx tailwindcss -i ./styles/globals.css -o ./dist/output.css --watch",
		"build:css": "npx tailwindcss -i ./styles/globals.css -o ./dist/output.css --minify",
		"up": "docker-compose up --build",
		"down": "docker-compose down && echo y | docker image prune"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.4",
		"@heroicons/react": "^2.0.13",
		"@prisma/client": "^4.5.0",
		"@tanstack/react-query": "^4.14.5",
		"@types/node": "18.11.8",
		"@types/react": "18.0.24",
		"@types/react-dom": "18.0.8",
		"axios": "^1.1.3",
		"lodash": "^4.17.21",
		"next": "13.0.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.39.4",
		"typescript": "4.8.4",
		"zustand": "^4.1.4"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.5.3",
		"@types/lodash": "^4.14.187",
		"autoprefixer": "^10.4.13",
		"eslint": "8.26.0",
		"eslint-config-next": "13.0.1",
		"postcss": "^8.4.18",
		"prisma": "^4.5.0",
		"tailwindcss": "^3.2.1"
	}
}
